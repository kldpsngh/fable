{
  "name": "fable",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build:prod": "webpack --config webpack.config.js --progress",
    "start": "concurrently -r -n \"Webpack,Nodemon\" \"npm run webpack-watch\" \"npm run server-watch\"",
    "lint:fix": "npm run prettier && eslint \"*/**/*.ts\" --quiet --fix",
    "lint": "eslint \"*/**/*.ts\" --quiet",
    "prettier": "prettier --write \"**/*.{ts,js}\"",
    "server-watch": "nodemon --max-old-space-size=4096 -e js dist/main.js",
    "webpack-watch": "webpack --config webpack.config.js --watch --progress",
    "prepare": "husky install",
    "tests": "npx mocha --require ts-node/register --recursive tests/*.test.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "revert-last-migration": "npx sequelize-cli db:migrate:undo",
    "build-models": "rm -rf dist models/pgsql && ts-node generate-models.ts && tsc",
    "postinstall": "npm run migrate && npm run build-models && npm run build:prod"
  },
  "author": "Kuldeep singh",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "reserved-words": "^0.1.2",
    "sequelize": "6.6.5",
    "sequelize-auto": "0.8.7",
    "sequelize-cli": "^6.2.0",
    "sequelize-cli-typescript": "^3.2.0-c",
    "source-map-support": "^0.5.21",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.13",
    "@types/mocha": "^10.0.1",
    "@types/node": "^17.0.8",
    "@types/redis": "^2.8.32",
    "@types/sequelize": "^4.28.10",
    "@types/source-map-support": "^0.5.4",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "^4.3.7",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "mocha": "^10.2.0",
    "prettier": "^2.3.2",
    "supertest": "^6.3.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.3.5",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  }
}
